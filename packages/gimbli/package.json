{
  "name": "gimbil",
  "version": "0.0.1",
  "description": "Gimbil - Reserved package",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["gimbli"],
  "author": "Commet Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "24.10.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
