{
  "name": "commet",
  "version": "1.0.0",
  "description": "Commet CLI - Manage your billing platform from the command line",
  "bin": {
    "commet": "./bin/commet"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "billing",
    "cli",
    "typescript",
    "commet"
  ],
  "author": "Commet Team",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "8.0.1",
    "chalk": "5.6.2",
    "commander": "14.0.2",
    "jsonc-parser": "3.3.1",
    "open": "11.0.0",
    "ora": "9.0.0"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://commet.co",
  "repository": {
    "type": "git",
    "url": "https://github.com/commet-labs/commet.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
