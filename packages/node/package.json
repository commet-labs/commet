{
  "name": "@commet/node",
  "version": "1.1.0",
  "description": "Commet SDK for Node.js - Billing and usage tracking",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "billing",
    "sdk",
    "typescript",
    "payments",
    "invoicing",
    "usage-based-billing",
    "seat-licensing"
  ],
  "author": "Commet Team",
  "license": "MIT",
  "dependencies": {
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://commet.co",
  "repository": {
    "type": "git",
    "url": "https://github.com/commet-labs/commet.git",
    "directory": "packages/node"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
